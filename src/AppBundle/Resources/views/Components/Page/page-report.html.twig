{% extends 'AppBundle:Layouts:application.html.twig' %}
{% block htmlTitle %}Deputyship report{% endblock %}
{% block contentClass %}page-report-content{% endblock %}

{% block pageContent %}

    <a name="pageBody"></a>

    {% block pageNavigation %}
        {% include 'AppBundle:Components/Navigation:_report-nav.html.twig' with {
            'report': report
        } %}
    {% endblock %}

    <h1 class="heading-large">{% block pageTitle %}{% endblock %}</h1>

    <div id="page-body">
        <div id="{% block leftContentId %}pagebody-left-content{% endblock %}">
           {% block pageBody %}{% endblock %}
        </div>
        <div id="body-footer-nav">{% block pageBodyFooterNav %}{% endblock %}</div>
    </div>

{% endblock %}
